{
  "name": "calculator",
  "version": "1.0.0",
  "description": "Простой, но мощный калькулятор с научными функциями.",
  "main": "script.js",
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "jest --watch --verbose",
    "test:divide": "jest --testNamePattern=\"ТЕСТИРОВАНИЕ ФУНКЦИИ ДЕЛЕНИЯ\" --verbose",
    "test:multiply": "jest --testNamePattern=\"ТЕСТИРОВАНИЕ ФУНКЦИИ УМНОЖЕНИЯ\" --verbose",
    "test:sqrt": "jest --testNamePattern=\"ТЕСТИРОВАНИЕ ФУНКЦИИ КВАДРАТНОГО КОРНЯ\" --verbose",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lladrvvne/Calculator.git"
  },
  "keywords": ["calculator", "jest", "unit-testing"],
  "author": "",
